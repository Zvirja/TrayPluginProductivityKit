      <action>
        <type>TrayPluginProductivityKit.SuperClick.SpecializedHandlers.FreeHandlers.IISReset</type>
        <alias>IISReset</alias>
        <format>don't use</format>
        <description>empty</description>
      </action>
      <action>
        <type>TrayPluginProductivityKit.SuperClick.SpecializedHandlers.FreeHandlers.InstallInstance</type>
        <alias>InstallInstance</alias>
        <format>don't use</format>
        <description>empty</description>
      </action>
      <action>
